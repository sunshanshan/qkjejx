		</action>
		
		<action name="check_360ScoreAdd" class="com.qkj.check360.action.ScoreAction" method="add">
		</action>
		<action name="check_360ScoreSave" class="com.qkj.check360.action.ScoreAction" method="save">
		</action>
		
	
	</package>
</struts>